version = 3
server_host = {{sogo_maildomain}}:389
search_base = {{sogo_postfix_search_base}}
timeout = 3
scope = sub
query_filter = (&(mail=%s)(sAMAccountType=268435456))
result_filter = %s
result_attribute = mail
special_result_attribute = member
bind = yes
bind_dn = {{sogo_postfix_vmail_basedn}}
bind_pw = {{sogo_vmail_password}}



